/* component/nav-bar/nav-bar.wxss */
@import'../../iconfont.wxss';
navigator{
  color: inherit;
  cursor: default;
}
.suspension{
  height: 168rpx;
}
.suspension-box{
  position: fixed;
  width: 100%;
  z-index: 100;
  background-color: #fff;
  top: 0;
}
.channel-menu{
  display: flex;
}
.m-home{
  background: #fff;
}
.m-navbar{
  height: 88rpx;
  line-height: 88rpx;
  padding: 0rpx 24rpx 0rpx 36rpx;
  font-size: 0;
  background: #fff;
}
.m-navbar .logo{
  float: left;
  height: 88rpx;
}
.m-navbar .right{
  float: right;
  height: 88rpx;
}
.m-navbar .logo i{
  color: #fb7299;
  font-size: 56rpx;
}
.Navbar_logo::before{
  content: "\e600";
}
.m-navbar .right .search{
  margin-right: 44rpx;
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  margin-top: 4rpx;
}
.m-navbar .right .search i{
  color: #ccc;
  font-size: 44rpx;
}
.ic_search_tab::before{
  content: "\e601";
}
.m-navbar .right .face{
  vertical-align: top;
  width: 48rpx;
  height: 48rpx;
  display: inline-block;
  margin-top: 20rpx;
}
.m-navbar .right .face image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background:  no-repeat 50%;
  background-size: contain;
  background-color: #e7e7e7;
}
.m-navbar .right .openapp{
  float: right;
  padding: 0rpx;
  margin-left: 24rpx;
  width: 160rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin-top: 20rpx;
  background: #fb7299;
  border: none;
  border-radius: 4rpx;
  color: #fff;
  font-size: 24rpx;
  vertical-align: top;
  outline: none;
  cursor: default;
  user-select: none;
}
